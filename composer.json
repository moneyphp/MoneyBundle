{
    "name": "moneyphp/money-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for the popular Money library",
    "license": "MIT",
    "keywords": ["money"],
    "homepage": "https://github.com/moneyphp/MoneyBundle",
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^4.0",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Money\\MoneyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Money\\MoneyBundle\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
