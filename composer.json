{
    "name": "moneyphp/money-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for the popular Money library",
    "license": "MIT",
    "keywords": ["money"],
    "homepage": "https://github.com/moneyphp/MoneyBundle",
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "^2.7|^3.0",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "symfony/symfony": "^2.7|^3.0",
        "phpunit/phpunit": "^4.4",
        "matthiasnoback/symfony-config-test": "^1.4",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.6"
    },
    "autoload": {
        "psr-4": {
            "Money\\MoneyBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Money\\MoneyBundle\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
